{
    "name": "kaelynlayne/ipinfo",
    "description": "find your ip address and do things",
    "keywords": [
        "universal package layout",
        "standard package layout",
        "IPInfo",
        "project layout"
    ],
    "homepage": "https://www.phpexperts.pro/",
    "license": "MIT",
    "authors": [
        {
            "name": "TDB, inc"
        },
        {
            "name": "Kaelyn L. Quinn"
            
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*|8.*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "*",
        "symfony/var-dumper": "*",
        "povils/phpmnd": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Kaelynlayne\\IPInfo\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Kaelynlayne\\IPInfo\\Tests\\" : "tests/" }
    },
    "config": {
        "classmap-authoritative": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php install.php"
        ]
    }
}
